<svg width="128" height="128" viewBox="0 0 128 128" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="grad128" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#6366f1;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="highlight128" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:rgba(255,255,255,0.3);stop-opacity:1" />
      <stop offset="100%" style="stop-color:rgba(255,255,255,0.1);stop-opacity:1" />
    </linearGradient>
    <filter id="shadow128" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="0" dy="4" stdDeviation="4" flood-color="rgba(0,0,0,0.3)"/>
    </filter>
    <filter id="glow" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur stdDeviation="2" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background circle with shadow -->
  <circle cx="64" cy="64" r="60" fill="url(#grad128)" filter="url(#shadow128)"/>
  
  <!-- Inner highlight circle -->
  <circle cx="64" cy="64" r="56" fill="url(#highlight128)"/>
  
  <!-- Inner border -->
  <circle cx="64" cy="64" r="54" fill="none" stroke="rgba(255,255,255,0.2)" stroke-width="1"/>
  
  <!-- Tab groups representation with better spacing and design -->
  
  <!-- Group 1 - Left side (White/Silver tabs representing ungrouped) -->
  <g opacity="0.95">
    <rect x="16" y="30" width="22" height="10" rx="2" fill="white" opacity="0.95" filter="url(#glow)"/>
    <rect x="16" y="42" width="22" height="10" rx="2" fill="white" opacity="0.8"/>
    <rect x="16" y="54" width="22" height="10" rx="2" fill="white" opacity="0.65"/>
    <rect x="16" y="66" width="22" height="10" rx="2" fill="white" opacity="0.5"/>
    <rect x="16" y="78" width="22" height="10" rx="2" fill="white" opacity="0.35"/>
    <rect x="16" y="90" width="22" height="10" rx="2" fill="white" opacity="0.2"/>
  </g>
  
  <!-- Group 2 - Center (Green tabs representing work/productivity) -->
  <g opacity="0.95">
    <rect x="42" y="30" width="22" height="10" rx="2" fill="#22c55e" filter="url(#glow)"/>
    <rect x="42" y="42" width="22" height="10" rx="2" fill="#22c55e" opacity="0.9"/>
    <rect x="42" y="54" width="22" height="10" rx="2" fill="#22c55e" opacity="0.75"/>
    <rect x="42" y="66" width="22" height="10" rx="2" fill="#22c55e" opacity="0.6"/>
  </g>
  
  <!-- Group 3 - Right side (Orange/Red tabs representing entertainment) -->
  <g opacity="0.95">
    <rect x="68" y="30" width="22" height="10" rx="2" fill="#f59e0b" filter="url(#glow)"/>
    <rect x="68" y="42" width="22" height="10" rx="2" fill="#f59e0b" opacity="0.9"/>
    <rect x="68" y="54" width="22" height="10" rx="2" fill="#f59e0b" opacity="0.75"/>
    <rect x="68" y="66" width="22" height="10" rx="2" fill="#f59e0b" opacity="0.6"/>
    <rect x="68" y="78" width="22" height="10" rx="2" fill="#f59e0b" opacity="0.45"/>
    <rect x="68" y="90" width="22" height="10" rx="2" fill="#f59e0b" opacity="0.3"/>
    <rect x="68" y="102" width="22" height="10" rx="2" fill="#f59e0b" opacity="0.15"/>
  </g>
  
  <!-- Group 4 - Far right (Purple tabs representing development) -->
  <g opacity="0.95">
    <rect x="94" y="30" width="22" height="10" rx="2" fill="#8b5cf6" filter="url(#glow)"/>
    <rect x="94" y="42" width="22" height="10" rx="2" fill="#8b5cf6" opacity="0.85"/>
    <rect x="94" y="54" width="22" height="10" rx="2" fill="#8b5cf6" opacity="0.7"/>
  </g>
  
  <!-- Group connectors with glow effect -->
  <g opacity="0.8">
    <rect x="38" y="32" width="4" height="6" rx="1" fill="rgba(255,255,255,0.9)" filter="url(#glow)"/>
    <rect x="64" y="32" width="4" height="6" rx="1" fill="rgba(255,255,255,0.9)" filter="url(#glow)"/>
    <rect x="90" y="32" width="4" height="6" rx="1" fill="rgba(255,255,255,0.9)" filter="url(#glow)"/>
  </g>
  
  <!-- Organizing arrows at the top -->
  <g opacity="0.9" filter="url(#glow)">
    <path d="M20 20 L30 20 L25 15 Z" fill="rgba(255,255,255,0.9)"/>
    <path d="M46 20 L56 20 L51 15 Z" fill="rgba(255,255,255,0.9)"/>
    <path d="M72 20 L82 20 L77 15 Z" fill="rgba(255,255,255,0.9)"/>
    <path d="M98 20 L108 20 L103 15 Z" fill="rgba(255,255,255,0.9)"/>
  </g>
  
  <!-- Central organizing symbol -->
  <circle cx="64" cy="20" r="4" fill="rgba(255,255,255,0.9)" filter="url(#glow)"/>
  <path d="M62 18 L66 18 M64 16 L64 20 M62 22 L66 22" stroke="url(#grad128)" stroke-width="1.5" stroke-linecap="round"/>
</svg>